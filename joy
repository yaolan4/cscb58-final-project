
module joy(LEDR, GPIO, CLOCK_50);
		input [10:8]GPIO;
		output [9:0]LEDR;
		input CLOCK_50;
		reg [0:0]out;
		reg[0:0]x, y;
		
		always@(posedge CLOCK_50)
		begin
//			if(GPIO[10] > 3'd900 || GPIO[10] < 3'd100)//x
//				x <= 1'b1;
//			else if(GPIO[8] > 3'd900 || GPIO[8] < 3'd100)//y
//				y <= 1'b1;
//			else
				begin
					x <= 1'b0;
					y <=1'b0;
				end
		end
		
		assign LEDR[0] = x;
		assign LEDR[3] = y;
		assign LEDR[6] = GPIO[9];//sw


endmodule

